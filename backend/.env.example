# Rename to .env and fill in values
SECRET_KEY=replace-with-a-random-long-string
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
# Where Google sends the user back after consent
OAUTH_REDIRECT_URL=http://localhost:8000/api/auth/callback
# Where to redirect the user after login completes
FRONTEND_AFTER_LOGIN=http://localhost:8000/
# CORS origin for frontend (comma separated list)
CORS_ORIGINS=http://localhost:8000,http://127.0.0.1:8000
